{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "dom",
      "es6",
      "es2017",
    ],
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./client/dist/",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "jsx": "react",
    "allowJs": true,
  }
}